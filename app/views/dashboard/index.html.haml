
%div.dashboard
  .row-fluid.dashboard-main-header-row
    .span12
      %h2#dashboard-header.span6 Dashboard
      .span2.main-category#dashboard-manage
        %a{href: '#'} Manage
      .span2.main-category#dashboard-schedule
        %a{href: '#'} Schedule
      .span2.main-category#dashboard-stats
        %a{href: '#'} Stats
  .row-fluid.dashboard-manage-subcategory-row
    .span12
      .span6#workout-main.dashboard-active-subcategory
        %a{href: "#"} Main
      .span6#past-workouts.dashboard-inactive-subcategory
        %a{href: '#'} Past Workouts
  .row-fluid.dashboard-upcoming-workouts-row
    .span12
      .span3.offset1
        %p Upcoming workouts:
  .row-fluid#for-side-borders
    #workout-form-container
  .row-fluid.dashboard-add-workout-button-row
    .span12.add-button-secondary-container
      .span4.offset4.add-workout-button-area






